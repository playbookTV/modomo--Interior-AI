{
  "name": "reroom-cloud-backend",
  "version": "2.0.0",
  "description": "ReRoom Cloud Backend - Railway hosted microservice with Supabase, Cloudflare R2, and RunPod integration",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "tsx src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "postinstall": "npm run build"
  },
  "keywords": [
    "reroom",
    "ai",
    "interior-design",
    "photo-processing",
    "cloudflare-r2",
    "supabase",
    "runpod",
    "clerk",
    "railway"
  ],
  "author": "ReRoom Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@supabase/supabase-js": "^2.53.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reroom/backend"
  },
  "bugs": {
    "url": "https://github.com/reroom/backend/issues"
  },
  "homepage": "https://reroom.app"
}